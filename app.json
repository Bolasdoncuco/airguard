{
  "expo": {
    "name": "AirGuard",
    "slug": "airguard",
    "version": "1.0.0",
    "scheme": "airguard",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",  
    "newArchEnabled": true,

    "android": {
      "package": "com.anonymous.airguard",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",  
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS"
      ],
      "edgeToEdgeEnabled": true
    },

    "ios": {
      "supportsTablet": true
    },

    "plugins": [
      "expo-notifications",
      "expo-router",
      [
        "expo-splash-screen"
      ],
      "react-native-ble-plx",
      "react-native-wifi-reborn",
      "expo-web-browser"
    ],

    "extra": {
      "router": {},
      "eas": {
        "projectId": "d1c157ff-27cc-4a06-afff-eb1c069f1cdc"
      }
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "owner": "bolasdoncuco"
  }
}
